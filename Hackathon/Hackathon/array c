#include <stdio.h>

// Function to compute and print the summation of two arrays
void sumArrays(int arr1[], int arr2[], int length) {
    int sum[length];  // new array to store result

    // Compute element-wise sum
    for (int i = 0; i < length; i++) {
        sum[i] = arr1[i] + arr2[i];
    }

    // Print the resulting array
    printf("Sum of the two arrays:\n");
    for (int i = 0; i < length; i++) {
        printf("%d ", sum[i]);
    }
    printf("\n");
}

int main() {
    int arr1[] = {1, 2, 3, 4, 5};
    int arr2[] = {5, 4, 3, 2, 1};
    int length = sizeof(arr1) / sizeof(arr1[0]);  // find array length

    // Call the function
    sumArrays(arr1, arr2, length);

    return 0;